<BoxPane styles="{fill:true,padding:10}" xmlns:bxml="http://pivot.apache.org/bxml" xmlns="org.apache.pivot.wtk" xmlns:content="org.apache.pivot.wtk.content"
	orientation="vertical">
	<BoxPane orientation="vertical" styles="{padding:10}">
		<Label text="Pandora API Wiki" styles="{font:{bold:true}}" />
		<LinkButton action="openURL">
			<content:ButtonData text="http://pan-do-ra-api.wikia.com/wiki/Pan-do-ra_API_Wiki" />
			<userData url="http://pan-do-ra-api.wikia.com/wiki/Pan-do-ra_API_Wiki" />
		</LinkButton>
	</BoxPane>
	<BoxPane orientation="vertical" styles="{padding:10}">
		<Label text="Skip icon designed by Marc Antosch from the Noun Project " styles="{font:{bold:true}}" />
		<LinkButton action="openURL">
			<content:ButtonData text="http://thenounproject.com/noun/skip/#icon-No5650" />
			<userData url="http://thenounproject.com/noun/skip/#icon-No5650" />
		</LinkButton>
	</BoxPane>
	<BoxPane orientation="vertical" styles="{padding:10}">
		<Label text="Disconnect icon designed by Danny Amacher from the Noun Project " styles="{font:{bold:true}}" />
		<LinkButton action="openURL">
			<content:ButtonData text="http://thenounproject.com/noun/disconnect/#icon-No6446" />
			<userData url="http://thenounproject.com/noun/disconnect/#icon-No6446" />
		</LinkButton>
	</BoxPane>
	<BoxPane orientation="vertical" styles="{padding:10}">
		<Label text="Beluga picture by Opals-On-BlackCom" styles="{font:{bold:true}}" />
		<Label text="(Creative Commons Attribution 2.0 Generic license)" styles="{font:{bold:true}}" />
		<LinkButton action="openURL">
			<content:ButtonData text="http://www.flickr.com/photos/opals-on-black" />
			<userData url="http://www.flickr.com/photos/opals-on-black" />
		</LinkButton>
	</BoxPane>
</BoxPane>