<div class="result">
	<a href="command://add-station/search/$item.getMusicToken()">
	#if ($item.class.name == "info.bonjean.beluga.response.SearchSong")
		<span class="song">
			$item.getSongName()
		</span>
	#end
	<span class="artist">
		#if ($item.class.name == "info.bonjean.beluga.response.SearchSong")
			$text['by']
		#end
		$item.getArtistName()
	</span>
	#if ($qualified)
		<span class='type'>
			#if ($item.class.name == "info.bonjean.beluga.response.SearchSong")
				($text['track'])
			#else
				($text['artist'])
			#end
		</span>
	#end
	</a>
</div>